<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!--Colors-->
    <SolidColorBrush x:Key="Background" Color="#FF282A46"/>

    <SolidColorBrush x:Key="TextForeground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="NotSelectedIcon" Color="#C7C7C7"/>
    <SolidColorBrush x:Key="SelectedIcon" Color="#FF9614"/>
    <SolidColorBrush x:Key="ButtonBackground" Color="#404040"/>
    <SolidColorBrush x:Key="ButtonBorder" Color="#606060"/>
    
    <SolidColorBrush x:Key="PitchColor" Color="#FF5542"/>
    <SolidColorBrush x:Key="VolumeColor" Color="#FF13B94B"/>
    <SolidColorBrush x:Key="PanningColor" Color="#FF366FE0"/>
    <SolidColorBrush x:Key="DisabledColor" Color="#808080"/>

    <SolidColorBrush x:Key="GeneratedColor" Color="#FF23FF56"/>

    <!--Values-->
    <sys:Double x:Key="ComposerTrackHeaderWidth">160</sys:Double>


</ResourceDictionary>